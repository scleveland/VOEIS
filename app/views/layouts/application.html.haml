      dojo.require("yogo.ui.MenuLinkBlankTarget");
      //dojo.require("yogo.maps.google");    
    
      //yogo.maps.google.load();
    
      var hideLoader = function(){
          dojo.fadeOut({
              node:"preloader",
              onEnd: function(){
                  dojo.style("preloader", "display", "none");
              }
          }).play();
      }
      
      $(window).ready(function(){ 
          if(window.pre_dojo_parse) pre_dojo_parse();
          dojo.parser.parse();
          if(window.post_dojo_parse) post_dojo_parse();
          //hideLoader();
      });

