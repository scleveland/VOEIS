            :javascript
              $(document).ready(function() {
              assignTimestamp(#{col.to_s});
              });
          -elsif ds_col.name == "VerticalOffset"
            :javascript
              $(document).ready(function() {
              assignVerticalOffset(#{col.to_s});
              });
          -elsif ds_col.name == "EndingVerticalOffset"
            :javascript
              $(document).ready(function() {
              assignEndingVerticalOffset(#{col.to_s});
              });
          -elsif ds_col.name == "Ignore" || ds_col.name == "ignore"
            :javascript
              $(document).ready(function() {
              assignIgnore(#{col.to_s});
              });
