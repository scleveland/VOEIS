  function updateProgressBar(){
  jsProgress.update({
  maximum: #{@columns.length + 1},
  progress: jsProgress.progress +1
  });
  if (jsProgress.progress == jsProgress.maximum){
    dijit.byId("save_data_button").set("disabled",false);
    dijit.byId("save_data_button").focus();
  }
  }

